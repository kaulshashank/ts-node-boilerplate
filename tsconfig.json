{
	"ts-node": {
		"files": true
	},
	"compilerOptions": {
		"forceConsistentCasingInFileNames": true,
		"target": "es5",
		"module": "commonjs",
		"lib": ["es5", "es6"],
		"sourceMap": true,
		"outDir": "dist",
		"rootDir": "src",
		"strict": true,
		"noImplicitAny": true,
		"strictNullChecks": true,
		"moduleResolution": "node",
		"esModuleInterop": true
	},
	"exclude": ["node_modules"],
	"include": ["src", "global.d.ts", "thirdparty.d.ts"]
}
